{
  "name": "@k4ys4r/wc-clock",
  "version": "1.0.1",
  "description": "a web component starter Vite + JS",
  "type": "module",
  "files": [
    "dist",
    "types"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/wc-clock.umd.cjs",
  "module": "./dist/wc-clock.js",
  "types": "./types/wc-clock.d.ts",
  "exports": {
    ".": {
      "import": "./dist/wc-clock.js",
      "require": "./dist/wc-clock.umd.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/K4ys4r/wc-clock.git"
  },
  "bugs": {
    "url": "https://github.com/k4ys4r/wc-clock/issues"
  },
  "homepage": "https://k4ys4r.github.io/wc-clock/",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "generate-dts": "tsc",
    "test": "vitest",
    "clean": "rm -rf dist node_modules types"
  },
  "author": "Hilal Balout <k4ys4r.webdev@hotmail.com>",
  "license": "MIT",
  "devDependencies": {
    "jsdom": "^24.0.0",
    "typescript": "^5.3.3",
    "vite": "^4.3.5",
    "vitest": "^1.3.1"
  },
  "keywords": [
    "web components",
    "javascript",
    "wc",
    "custom-elements",
    "date",
    "time"
  ]
}
